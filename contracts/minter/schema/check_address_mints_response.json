{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CheckAddressMintsResponse",
  "description": "Used as execution msg and query response for single Address-Value pairs eg: address has 4 mints",
  "type": "object",
  "required": [
    "address",
    "value"
  ],
  "properties": {
    "address": {
      "description": "address for the promised values",
      "type": "string"
    },
    "value": {
      "description": "mint count",
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  },
  "additionalProperties": false
}
